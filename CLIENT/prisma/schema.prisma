// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Client{
  id              String        @id @default(uuid())
  username        String        @unique
  password        String        
  name            String
  address         String
  birthday        String
  cpf             String
  created_at      DateTime      @default(now())

  @@map("clients")
}

